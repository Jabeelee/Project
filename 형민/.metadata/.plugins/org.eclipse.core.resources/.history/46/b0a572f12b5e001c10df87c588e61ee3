package com.camflex.client.login.dao;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import com.camflex.client.login.vo.LoginVO;

@Repository
public interface LoginDAO {
	
	@Autowired
	private SqlSessionTemplate sqlSession;
	
	public void insertMember(LoginVO vo)  {
		sqlSession.insert("LoginDAO.insertMember", vo);
	}

	public LoginVO idCheck(String m_id) {
		LoginVO Check = sqlSession.selectOne("LoginDAO.idCheck", m_id)
	}
}
