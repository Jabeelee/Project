<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.camflex.client.login.dao.LoginDAO">
	<!-- 로그인 처리 -->
	<select id="Check"
		resultType="com.camflex.client.login.vo.LoginVO">
		select *
		from member_tbl
		where m_id=#{m_id} and m_pw=#{m_pw}
	</select>

	<!-- 회원가입 처리 -->
	<insert id="insert"
		parameterType="com.camflex.client.login.vo.LoginVO">
		<selectKey keyProperty="m_number" resultType="int"
			order="BEFORE">
			SELECT member_tbl_seq.nextval FROM DUAL
		</selectKey>

		insert into member_tbl
		(m_id, m_pw, m_name, m_birth,m_phone,m_number)
		values
		(#{m_id},#{m_pw},#{m_name},#{m_birth},#{m_phone},#{m_number})


	</insert>


</mapper>