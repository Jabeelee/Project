<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.camflex.client.login.dao.LoginDAO">
	<!-- 로그인 처리 -->
	<select id="loginAccess" resultType="String">
		select
		m_id, m_pw
		from
		member_tbl
		where m_id = #{m_id} AND m_pw
		=#{m_pw}
	</select>

	<!-- 회원가입 처리 -->
	<insert id="insert"
		parameterType="com.camflex.client.login.vo.LoginVO">
		insert into member_tbl
		(m_id, m_pw, m_name, m_birth,
		m_phone)
		values
		(#{m_id},#{m_pw},#{m_name},#{m_birth},#{m_phone})
	</insert>


</mapper>