<mapper namespace="com.camflex.client.login.dao.LoginDAO">
	
	
	
	<!-- 로그인 처리 -->
	<select id="loginCheck"
		resultType="com.camflex.client.login.vo.LoginVO">
		select m_id, m_pw
		from member_tbl
		where m_id=#{m_id} and
		m_pw=#{m_pw}
	</select>




