<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.camflex.client.login.dao.LoginDAO">
	<!-- 로그인 처리 -->
	<select id="Check"
		resultType="com.camflex.client.login.vo.LoginVO">
		select *
		from member_tbl
		where m_id=#{m_id} and m_pw=#{m_pw}
	</select>

	<!-- 회원가입 처리 -->
	<insert id="regNotice" parameterType="notice">
		<selectKey keyProperty="n_number" resultType="int"
			order="BEFORE">
			SELECT notice_tbl_seq.nextval FROM DUAL
		</selectKey>

	</insert>



</mapper>