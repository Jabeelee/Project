<mapper namespace="com.spring.client.login.dao.LoginDAO">
	
	<!-- 회원가입 -->
	<insert id="insertMember" parameterType="login">
		insert into member_TBL (m_id, m_pw, m_name, m_birth, m_phone)
		value (#{m_id}, #{m_pw}, #{m_name}, #{birth}, #{m_name})
	</insert>
	
	

</mapper>