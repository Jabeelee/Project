<mapper namespace="com.camflex.client.login.dao.LoginDAO">

	<!-- <resultMap type="member" id="memberMap"> <result property="m_id" column="m_id"/> 
		<result property="m_pw" column="m_pw"/> <result property="m_name" column="m_name"/> 
		<result property="m_phone" column="m_phone"/> <result property="m_birth" 
		column="m_birth"/> <result property="m_date" column="m_date"/> <result property="m_number" 
		column="m_number"/> </resultMap> -->

	<!-- 로그인 사용자ID -->
	<select id="userIdSelect" parameterType="login"
		resultType="login">
		Select m_id,m_pw
		from member_tbl
		where m_id=#{m_id}
	</select>

	<!-- 로그인 ID,Pw -->
	<select id="loginSelect" parameterType="login"
		resultType="login">
		select m_id,m_pw,m_name
		from member_tbl
		where m_id=#{m_id}
		and
		m_pw=#{m_pw}
	</select>




</mapper>